@Pass{
    FS_PREC(mediump,float)
    @Disable(CULL_FACE)
    varying vec2 vTexCoord;
    varying float vTime;

#ifdef VS
    @POSITION
    attribute vec3 position;
    @TEXCOORD
    attribute vec2 texcoord;
    uniform mat4 _matPVM;
    uniform float _time;

    void main(){
        vTexCoord = texcoord;
        float c = cos(_time/1000.0);
        float s = sin(_time/1000.0);
    
/*
        mat4 matAxisX = mat4(1.0);
        mat4 matAxisY = mat4(1.0);

        matAxisX[1][1] = c;
        matAxisX[1][2] = -s;
        matAxisX[2][1] = s;
        matAxisX[2][2] = c;
        
        matAxisY[0][0] = c;
        matAxisY[0][2] = s;
        matAxisY[2][0] = -s;
        matAxisY[2][2] = c;
*/
        //gl_Position = _matPVM * matAxisX * matAxisY * vec4(position, 1.0);
        gl_Position = _matPVM * vec4(position,1.0);
    }
#endif

#ifdef FS
    uniform sampler2D texture;
    void main(){
        vec3 diffuse = texture2D(texture, vTexCoord).rgb;
        gl_FragColor = vec4(diffuse, 1.0);
    }
#endif
}